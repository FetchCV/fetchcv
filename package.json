      "dotenv": "^16.4.5",
      "ejs": "^3.1.10",
      "express": "^4.21.1",
      "express-session": "^1.18.1",
      "mongoose": "^8.7.2",
      "node": "^21.7.3",
      "nvm": "^0.0.4",
      "tailwind": "^4.0.0"
